<library path="lib/libwrench_est_plugins">
    <class type="aerial_robot_control::WrenchEstNone" base_class_type="aerial_robot_control::WrenchEstActuatorMeasBase">
        <description>Wrench estimation always output zero.</description>
    </class>
    <class type="aerial_robot_control::WrenchEstMomentum" base_class_type="aerial_robot_control::WrenchEstActuatorMeasBase">
        <description>Wrench estimation using Momentum-based method.</description>
    </class>
    <class type="aerial_robot_control::WrenchEstMomAcc" base_class_type="aerial_robot_control::WrenchEstActuatorMeasBase">
        <description>Wrench estimation using Momentum-based method for torque and Acceleration-based method for force.</description>
    </class>
    <class type="aerial_robot_control::WrenchEstAcceleration" base_class_type="aerial_robot_control::WrenchEstActuatorMeasBase">
        <description>Wrench estimation using Acceleration-based method.</description>
    </class>

    <class type="aerial_robot_control::WrenchEstMHEAccMom" base_class_type="aerial_robot_control::WrenchEstActuatorMeasBase">
        <description>Wrench estimation using MHE based acc + mom method.</description>
    </class>

</library>
